{
  "expo": {
    "name": "my-app",
    "slug": "my-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": ".src/assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": ".src/assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": ".src/assets/logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": ".src/assets/logo.png"
    }
  }
}
